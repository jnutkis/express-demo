<div class="row">
  {{#each names}}
  <div class="col s12 m12 l6">
    <div class="card grey lighten-4">
      <div class="card-content">
        <span class="card-title center-align">{{this.name}}</span>
        <p>{{this.data}}</p>
      </div>
      <div class="card-action">
        <form method="POST" action="/mongoose/{{this.id}}?_method=DELETE">
          <a href="/mongoose/{{this.id}}" class="btn waves-effect waves-light blue-grey">See Item</a>
          <button type="submit" class="btn red darken-2 waves-effect waves-light right">Delete</button>
        </form>
      </div>
    </div>
  </div>
  {{/each}}
</div>